<section class="container places-list">
  <div><ul>
    {{#each places}}
    <li>The {{ this.type }} "{{ this.name }}" is at</li>
    {{/each}}
  </ul>
  </div>
</section>
<section class="jumbotron">
  <style>
    #gmap {
      height: 400px;
      width: 100%;
      box-sizing: content-box;
    }
  </style>
  
  <div id="gmap"></div>
  
  <script>
    function initMap() {
      const naples = {lat: 40.8397706, lng: 14.2216924};
      const map = new google.maps.Map(
      document.getElementById('gmap'), {zoom: 14, center: naples});
      const marker = new google.maps.Marker({position: naples, map: map});
    }
  </script>
  <script async defer
    src="https://maps.googleapis.com/maps/api/js?key={{gMapAPIKey}}&callback=initMap">
  </script>
</section>
